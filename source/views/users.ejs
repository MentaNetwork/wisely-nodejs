<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Bienvenido a <%= title %></p>
    <h4>Timeout: <%= timeout %></h4>
    <ul>
      <% users.forEach(function(user) { %>
      <li><%= user.name %></li>
      <% }); %>
    </ul>
    <p>
      <b>Host: <%= hostname %></b>
    </p>
  </body>
</html>
